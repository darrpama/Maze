<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="GuiInterface.App"
             xmlns:local="using:GuiInterface"
             RequestedThemeVariant="Light">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="Controls/MazeControl.axaml"></StyleInclude>
        <StyleInclude Source="Controls/MazeCellControl.axaml"></StyleInclude>
        <StyleInclude Source="Controls/CaveControl.axaml"></StyleInclude>
        <StyleInclude Source="Controls/CaveCellControl.axaml"></StyleInclude>
    </Application.Styles>
</Application>